{
  "name": "knex-json-where",
  "version": "1.2.1",
  "description": "Add filters to a Knex query using a json. Great for use with REST or GraphQL APIs that need filtering on requests",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "link": "npm run build && npm run lint && npm run test && npm link",
    "publish": "npm run build && npm run lint && npm run test && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricardocrescenti/knex-json-where.git"
  },
  "keywords": [
    "knex",
    "json",
    "filter",
    "where",
    "condition",
    "graphql"
  ],
  "author": {
    "name": "Ricardo Crescenti",
    "email": "ricardo.crescenti@gmail.com",
    "url": "https://github.com/ricardocrescenti"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/ricardocrescenti/knex-json-where/issues"
  },
  "homepage": "https://github.com/ricardocrescenti/knex-json-where#readme",
  "dependencies": {
    "@types/knex": "^0.16.1",
    "knex": "^2.4.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "jest": "^25.2.3",
    "pg": "^7.18.2",
    "prettier": "^2.0.2",
    "ts-jest": "^25.2.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
